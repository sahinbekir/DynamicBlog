@model EntityLayer.Concrete.Message2
@{
    ViewData["Title"] = "MessageDetails";
    Layout = "~/Views/Shared/WriterLayout.cshtml";
}



<h1>Message Details</h1>


@Html.HiddenFor(x=>x.MessageId, new {@class="form-control"})
<br />
@Html.Label("Message Subject")
@Html.TextBoxFor(x=>x.MessageSubject, new {@class="form-control"})
<br />
@Html.Label("Message Context")
@Html.TextAreaFor(x=>x.MessageContext,4,3, new {@class="form-control"})
<br />
<a href="/Message/InBox/" class="btn btn-primary">Go To Main Page</a>


